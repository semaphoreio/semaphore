			"extensions": [
				"mjmcloug.vscode-elixir",
				"pantajoe.vscode-elixir-credo",
				"dbaeumer.vscode-eslint",
				"bierner.markdown-mermaid",
				"samuel-pordeus.elixir-test",
				"eamodio.gitlens",
				"rvest.vs-code-prettier-eslint",
				"openai.chatgpt",
