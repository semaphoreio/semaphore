{
	"name": "Semaphoreio - Front",
	"dockerComposeFile": [
		"../docker-compose.yml",
		"docker-compose.yml"
	],
	"service": "app",
	"workspaceFolder": "/workspace/${localWorkspaceFolderBasename}",
	"customizations": {
